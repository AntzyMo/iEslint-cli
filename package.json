{
  "name": "@antzy/ieslint",
  "version": "0.2.1",
  "description": "generate my eslint config",
  "main": "bin/ieslint.js",
  "type": "module",
  "bin": {
    "ies": "bin/ieslint.js"
  },
  "files": [
