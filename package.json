    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "diff": "^5.1.0",
    "execa": "^8.0.1",
